{
	"compilerOptions": {
		"target": "es2016",
		"module": "CommonJS",
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"baseUrl": ".",
		"paths": {
			"@lib/*": ["./src/lib/*"],
			"@models/*": ["./src/models/*"],
			"@queries/*": ["./src/queries/*"],
			"@repositories/*": ["./src/repositories/*"],
			"@routes/*": ["./src/routes/*"]
		}
	}
}
